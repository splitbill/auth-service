    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
